<h1>Create a new blogger</h1>
<%=form_with({model: Blogger.new}) do |f| %>
    <div>
        <label>name</label>
        <%=f.text_field(:name)%>
    </div>
    <div>
        <label>bio</label>
        <%=f.text_field(:bio)%>
    </div>
    <div>
        <label>age</label>
        <%=f.number_field(:age)%>
    </div>
    <%=f.submit("create blogger")%>

<%end%>